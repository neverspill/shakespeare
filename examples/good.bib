number = {22},
pages = {20178--20190},
publisher = {OSA},
title = {{Monte Carlo simulation of photon migration in 3D turbid media accelerated by graphics processing units.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2863034\&tool=pmcentrez\&rendertype=abstract},
volume = {17},
year = {2009}
}
