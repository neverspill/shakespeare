issn = {0947-6539},
journal = {Chemistry (Weinheim an der Bergstrasse, Germany)},
keywords = {crystalline approximant,high-temperature synthesis,nm length scale to,quasicrystal,result in,tantalum,tellurium,tiling at a 2},
month = jul,
number = {14},
pages = {3093--3102},
pmid = {12203339},
title = {{Ta97Te60: a crystalline approximant of a tantalum telluride quasicrystal with twelvefold rotational symmetry.}},
