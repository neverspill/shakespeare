year = {2013}
}
@article{Farhadi2013,
abstract = {We show that there are striking differences in the dynamical and structural properties of disks and ellipses subject to Couette shear at densities below the isotropic jamming point. In particular, sheared ellipses demonstrate exceptionally slow dynamical evolution that does not have a counterpart for disks. This slow dynamics appears in the mean velocity, local density, orientational order, and local stress. Starting from an unjammed state, ellipses first jam under shear, and then unjam due to small slow changes in their orientations. For jammed states, avalanche behavior, namely the building up and relaxation of stresses, is qualitatively different for disks and ellipses and leads to strikingly different stress rate dependence for ellipses vs. disks. Unlike the weak rate dependence typical of many granular systems, ellipses show power-law dependence on the shearing rate, \{$\backslash$Omega\}.},
author = {Farhadi, Somayeh and Behringer, Robert P.},
