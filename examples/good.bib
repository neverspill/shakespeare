title = {{On fluid-solid direct coexistence simulations: The pseudo-hard sphere model.}},
url = {http://scitation.aip.org.proxy.lib.umich.edu/content/aip/journal/jcp/139/14/10.1063/1.4823499},
volume = {139},
year = {2013}
}
