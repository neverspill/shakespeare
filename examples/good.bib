keywords = {Base Pairing,DNA, Single-Stranded,DNA, Single-Stranded: chemical synthesis,DNA, Single-Stranded: chemistry,Models, Chemical,Nanostructures,Nanostructures: chemistry,Nanotechnology,Nanotechnology: methods,Nucleotides,Nucleotides: chemistry},
number = {6111},
