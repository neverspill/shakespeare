number = {7},
pages = {569--74},
pmid = {24950314},
title = {{Chiral encoding may provide a simple solution to the origin of life.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24950314},
volume = {6},
year = {2014}
}
@article{Brown2013,
abstract = {Polystyrene particles partially coated with platinum and dispersed in H2O2 solution are often used as model self-propelled colloids. Current data suggest that propulsion is due to diffusiophoresis in a self-generated oxygen gradient around each particle. We find that the direction of propulsion is reversed on addition of an ionic surfactant, and that pH neutral salts reduce the propulsion speed whereas NaOH has no significant effect. We use these data, as well as measured reaction rates, to argue against propulsion by either neutral or ionic self-diffusiophoresis. However, our data are consistent with propulsion by a self-generated surface ionic current, which can derive from general asymmetries of the particle. We also show that rapid H2O2 consumption may render it impractical to perform 3D experiments at high particle concentrations using this system.},
author = {Brown, A. T. and Poon, W. C. K},
month = dec,
pages = {9},
title = {{How platinum-coated Janus particles swim}},
url = {http://arxiv.org/abs/1312.4130},
year = {2013}
}
