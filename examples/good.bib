journal = {The Journal of Chemical Physics},
number = {9},
pages = {94501},
pmid = {21384979},
title = {{Phase behavior of hard colloidal platelets using free energy calculations}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21384979},
volume = {134},
year = {2011}
}
